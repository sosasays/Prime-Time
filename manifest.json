{
  "manifest_version": 3,
  "name": "Prime Time",
  "description": "Manage your productivity with ease!",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": "timer.png"
  },
    "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "100": "images/icon-100.png"
  },
  "content_scripts": [
    {
      "js": ["main.js"]
    }
  ]
}